<?xml version="1.0" encoding="UTF-8"?>
<Module xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/BUTR/Bannerlord.XmlSchemas/master/SubModulePython.xsd" >
  <Id value="$moduleid$" />
  <Name value="$modulename$" />
  <Version value="v$version$" />
  <Official value="false" />
  <DefaultModule value="false" />
  <SingleplayerModule value="true" />
  <MultiplayerModule value="false" />
  <ModuleCategory value="Singleplayer" />
  <Url value="" />
  <DependedModules>
    <DependedModule Id="Bannerlord.Python" DependentVersion="v$pythonversion$" />
    <DependedModule Id="Native" DependentVersion="e$gameversion$.*" />
    <DependedModule Id="SandBoxCore" DependentVersion="e$gameversion$.*" />
    <DependedModule Id="Sandbox" DependentVersion="e$gameversion$.*" />
    <DependedModule Id="StoryMode" DependentVersion="e$gameversion$.*" />
    <DependedModule Id="CustomBattle" DependentVersion="e$gameversion$.*" />
  </DependedModules>
  <!-- Community Dependency Metadata -->
  <!-- https://github.com/BUTR/Bannerlord.BUTRLoader#for-modders -->
  <DependedModuleMetadatas>
    <DependedModuleMetadata id="Bannerlord.Python" order="LoadBeforeThis" version="v$pythonversion$" />

    <DependedModuleMetadata id="Native" order="LoadBeforeThis" optional="true" version="e$gameversion$.*" />
    <DependedModuleMetadata id="SandBoxCore" order="LoadBeforeThis" optional="true" version="e$gameversion$.*" />
    <DependedModuleMetadata id="Sandbox" order="LoadBeforeThis" optional="true" version="e$gameversion$.*" />
    <DependedModuleMetadata id="StoryMode" order="LoadBeforeThis" optional="true" version="e$gameversion$.*" />
    <DependedModuleMetadata id="CustomBattle" order="LoadBeforeThis" optional="true" version="e$gameversion$.*" />
  </DependedModuleMetadatas>
  <!-- Community Dependency Metadata -->
  <SubModules>
    <SubModulePython>
      <Name value="$moduleid$" />
      <ScriptName value="SubModule.py" />
      <SubModuleClassType value="SubModule" />
      <Tags />
    </SubModulePython>
  </SubModules>
</Module>
